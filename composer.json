{
    "name": "keyteqlabs/keymedia-ezpublish",
    "type": "ezpublish-legacy-extension",
    "description": "eZ Publish KeyMedia connectivity",
    "autoload": {
        "psr-0": {
            "keymedia\\": "../"
        },
        "classmap": [
            "operators/", "datatypes/"
        ]
    },
    "extra": {
        "installer-name": "keymedia",
        "ezpublish-legacy-extension-name": "keymedia"
    },
    "require": {
        "composer/installers": "*",
        "ezsystems/ezpublish-legacy-installer": "*",
        "keyteqlabs/ezote": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master@dev"
    }
}
